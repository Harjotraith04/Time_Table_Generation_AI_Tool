PORT=8000
NODE_ENV=development
MONGODB_URI=mongodb://localhost:27017/timetable_generator
JWT_SECRET=your_super_secret_jwt_key_here_change_in_production
JWT_EXPIRES_IN=7d

# Algorithm Configuration
GENETIC_ALGORITHM_POPULATION_SIZE=100
GENETIC_ALGORITHM_MAX_GENERATIONS=1000
GENETIC_ALGORITHM_CROSSOVER_RATE=0.8
GENETIC_ALGORITHM_MUTATION_RATE=0.1
GENETIC_ALGORITHM_ELITE_SIZE=10

# Constraints Configuration
MAX_HOURS_PER_DAY=8
MIN_BREAK_TIME=30
MAX_CONSECUTIVE_HOURS=3
LUNCH_BREAK_DURATION=60

# Performance Configuration
MAX_CONCURRENT_GENERATIONS=3
CACHE_RESULTS=true
CACHE_DURATION=3600

# Logging
LOG_LEVEL=info
LOG_FILE=logs/timetable_generator.log
